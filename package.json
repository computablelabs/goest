{
  "name": "goest",
  "version": "0.1.0",
  "description": "Golang testing sandbox for the Computable Protocol",
  "main": "index.js",
  "scripts": {
    "lint:solium": "node_modules/.bin/solium -d contracts",
    "compile:token:network": "~/go/bin/abigen -sol contracts/NetworkToken.sol -pkg networktoken -out contracts/networktoken/networktoken.go",
    "compile:token:market": "~/go/bin/abigen -sol contracts/MarketToken.sol -pkg markettoken -out contracts/markettoken/markettoken.go",
    "precompile:token:market:vyper": "vyper -f abi contracts/MarketToken.vy > contracts/markettoken/markettoken.abi && vyper -f bytecode contracts/MarketToken.vy > contracts/markettoken/markettoken.bin",
    "compile:token:market:vyper": "~/go/bin/abigen -abi contracts/markettoken/markettoken.abi -pkg markettoken -type MarketToken -out contracts/markettoken/markettoken.go -bin contracts/markettoken/markettoken.bin",
    "compile:token:ether": "~/go/bin/abigen -sol contracts/EtherToken.sol -pkg ethertoken -out contracts/ethertoken/ethertoken.go",
    "precompile:token:ether:vyper": "vyper -f abi contracts/EtherToken.vy > contracts/ethertoken/ethertoken.abi && vyper -f bytecode contracts/EtherToken.vy > contracts/ethertoken/ethertoken.bin",
    "compile:token:ether:vyper": "~/go/bin/abigen -abi contracts/ethertoken/ethertoken.abi -pkg ethertoken -type EtherToken -out contracts/ethertoken/ethertoken.go -bin contracts/ethertoken/ethertoken.bin",
    "compile:voting": "~/go/bin/abigen -sol contracts/Voting.sol -pkg voting -out contracts/voting/voting.go",
    "precompile:voting:vyper": "vyper -f abi contracts/Voting.vy > contracts/voting/voting.abi && vyper -f bytecode contracts/Voting.vy > contracts/voting/voting.bin",
    "compile:voting:vyper": "~/go/bin/abigen -abi contracts/voting/voting.abi -pkg voting -out contracts/voting/voting.go -bin contracts/voting/voting.bin",
    "compile:parameterizer": "~/go/bin/abigen -sol contracts/Parameterizer.sol -pkg parameterizer -out contracts/parameterizer/parameterizer.go",
    "compile:market": "~/go/bin/abigen -sol contracts/Market.sol -pkg market -out contracts/market/market.go",
    "test": "go test -v ./contracts/voting ./contracts/markettoken ./contracts/ethertoken"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/computablelabs/goest.git"
  },
  "keywords": [
    "Go",
    "golang",
    "Solidity",
    "Computable",
    "Badass"
  ],
  "author": "computable.io",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/computablelabs/goest/issues"
  },
  "homepage": "https://github.com/computablelabs/goest#readme",
  "devDependencies": {
    "solium": "^1.1.8"
  }
}
