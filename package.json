{
  "name": "goest",
  "version": "0.1.0",
  "description": "Golang testing sandbox for the Computable Protocol",
  "main": "index.js",
  "scripts": {
    "lint:solium": "node_modules/.bin/solium -d contracts",
    "compile:token:network": "~/go/bin/abigen -sol contracts/NetworkToken.sol -pkg networktoken -out contracts/networktoken/networktoken.go",
    "compile:token:market": "~/go/bin/abigen -sol contracts/MarketToken.sol -pkg markettoken -out contracts/markettoken/markettoken.go",
    "compile:token:ether": "~/go/bin/abigen -sol contracts/EtherToken.sol -pkg ethertoken -out contracts/ethertoken/ethertoken.go",
    "compile:voting": "~/go/bin/abigen -sol contracts/Voting.sol -pkg voting -out contracts/voting/voting.go",
    "compile:parameterizer": "~/go/bin/abigen -sol contracts/Parameterizer.sol -pkg parameterizer -out contracts/parameterizer/parameterizer.go",
    "compile:market": "~/go/bin/abigen -sol contracts/Market.sol -pkg market -out contracts/market/market.go",
    "compile:vyper:experiment": "vyper -f abi contracts/Experiment.v.py > contracts/experiment/experiment.abi && vyper -f bytecode contracts/Experiment.v.py > contracts/experiment/experiment.bin",
    "compile:experiment": "~/go/bin/abigen -abi contracts/experiment/experiment.abi -pkg experiment -out contracts/experiment/experiment.go -bin contracts/experiment/experiment.bin",
    "test": "go test -v ./..."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/computablelabs/goest.git"
  },
  "keywords": [
    "Go",
    "golang",
    "Solidity",
    "Computable",
    "Badass"
  ],
  "author": "computable.io",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/computablelabs/goest/issues"
  },
  "homepage": "https://github.com/computablelabs/goest#readme",
  "devDependencies": {
    "solium": "^1.1.8"
  }
}
